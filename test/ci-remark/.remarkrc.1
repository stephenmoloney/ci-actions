{
  "plugins": [
    "remark-preset-lint-consistent",
    "remark-preset-lint-markdown-style-guide",
    "remark-preset-lint-recommended",
    "remark-lint-no-dead-urls",
    "remark-validate-links",
    "remark-lint-no-url-trailing-slash",
    [
      "remark-lint-write-good",
      [
        "warn",
        {
          "passive": false,
          "so": true
        }
      ]
    ]
  ]
}
